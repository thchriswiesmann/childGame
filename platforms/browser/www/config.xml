<?xml version='1.0' encoding='utf-8'?>
<widget id="com.child.game" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Dentastic Adventure</name>
    <description>
        A fun game for Kids while waiting to get their teeth pulled out
    </description>
    <author email="christoph.wiesmann1@stud.th-deg.de" href="https://th-deg.de">
        Christoph Wiesmann
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon/tooth.png" />
</widget>
